{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": false,
    "max-line-length": {
      "options": [
        120
      ]
    },
    "no-var-requires": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "semicolon": [
      true,
      "never"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-namespace": false,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "space-before-function-paren": true,
    "trailing-comma": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}